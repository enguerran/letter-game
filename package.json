{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "a react boilerplate",
  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.0.0",
    "keymirror": "~0.1.0",
    "react": "^0.12.0"
  },
  "devDependencies": {
    "browserify": "latest",
    "envify": "latest",
    "live-server": "latest",
    "reactify": "latest",
    "uglify-js": "latest",
    "watchify": "latest"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "live-server & npm run watch",
    "watch": "watchify -o js/bundle.js -v -d . -e js/app.js",
    "test": "echo \"Error: no test specified\" && exit",
    "build": "NODE_ENV=production browserify . | uglifyjs -cm > js/bundle.min.js | npm run copy",
    "copy": "./copy.sh"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "author": "Enguerran Colson",
  "license": "MIT"
}
